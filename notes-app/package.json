{
  "name": "notes-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=notes-app:* nodemon --experimental-modules ./bin/www.mjs",
    "start-fs": "cross-env DEBUG=notes-app:* NOTES_MODEL=fs nodemon --experimental-modules ./bin/www.mjs ",
    "start-level": "cross-env DEBUG=notes-app:* NOTES_MODEL=level nodemon --experimental-modules ./bin/www.mjs ",
    "start-sqlite3": "cross-env DEBUG=notes-app:* SQLITE_FILE=chap07.sqlite3 NOTES_MODEL=sqlite3 nodemon --experimental-modules ./bin/www.mjs",
    "sqlite3-setup": "sqlite3 chap07.sqlite3 --init models/schema-sqlite3.sql"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cookie-parser": "~1.4.4",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "feather-icons": ">=4.5.x",
    "fs-extra": "^5.0.0",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "jquery": "^3.4.1",
    "level": "^5.0.1",
    "morgan": "~1.9.1",
    "popper.js": "^1.14.7",
    "rotating-file-stream": "^1.3.4",
    "sqlite3": "^4.0.9"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
